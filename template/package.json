    "accounting": "^0.4.1",
    "bitter-font": "^0.0.1",
    "bluebird": "^3.5.3",
    "boolean": "^1.0.0",
    "boom": "^7.3.0",
    "bootstrap": "4.3.1",
    "consolidate": "^0.15.1",
    "custom-fonts-in-emails": "^2.0.0",
    "dense": "^0.0.1",
    "email-templates": "^5.0.4",
    "font-awesome-assets": "^0.0.8",
    "frisbee": "^2.0.8",
    "gemoji": "^4.2.1",
    "highlight.js": "^9.15.6",
    "html5-history-api": "^4.2.10",
