    "@ladjs/i18n": "^1.1.0",
    "@ladjs/logger": "^0.1.0",
    "@ladjs/mongoose": "^0.1.2",
    "@ladjs/passport": "^0.0.1",
    "@ladjs/policies": "^0.0.1",
    "@ladjs/proxy": "^0.0.1",
