    "lodash": "^4.17.11",
    "mandarin": "^0.0.2",
    "max-listeners-exceeded-warning": "^0.0.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.19",
    "mongoose-common-plugin": "0.1.0",
    "mongoose-json-select": "^0.2.1",
    "mongoose-model-agenda": "^0.0.2",
    "mongoose-slug-plugin": "^1.0.3",
    "nodemailer": "^6.0.0",
    "nodemailer-base64-to-s3": "^1.0.0",
    "opn": "^5.5.0",
    "passport-local-mongoose": "^5.0.1",
    "pluralize": "^7.0.0",
    "popper.js": "^1.14.7",
    "pug": "^2.0.3",
    "qs": "^6.7.0",
    "randomstring-extended": "^1.0.0",
    "sanitize-html": "^1.20.0",
    "spinkit": "^1.2.5",
    "strength": "^0.1.4",
    "sweetalert2": "^8.6.0",
    "underscore.string": "^3.3.5",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@ladjs/babelify": "^8.0.1",
    "ava": "^1.4.0",
    "babel-plugin-lodash": "^3.3.4",
    "browserify": "^16.2.3",
    "caniuse-lite": "^1.0.30000951",
    "codecov": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-compat": "^3.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "event-stream": "^4.0.1",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "gulp": "4.x",
    "gulp-awspublish": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-cloudfront": "^0.0.14",
    "gulp-eslint": "^5.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-livereload": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-reporter": "^2.10.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-xo": "^0.21.0",
    "husky": "^1.3.1",
    "imagemin-pngquant": "^7.0.0",
    "lint-staged": "^8.1.5",
    "ms": "^2.1.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "nps": "^5.9.5",
    "nps-utils": "^1.7.0",
    "nyc": "^13.3.0",
    "postcss": "^7.0.14",
    "postcss-cssnext": "^3.1.0",
    "postcss-font-grabber": "^2.0.1",
    "postcss-font-magician": "^2.2.1",
    "postcss-inline-base64": "https://github.com/niftylettuce/postcss-inline-base64",
    "postcss-sassy-import": "^1.2.3",
    "postcss-unprefix": "^2.1.4",
    "prettier": "^1.16.4",
    "remark-cli": "^6.0.1",
    "remark-preset-github": "^0.0.13",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "xo": "^0.24.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "homepage": "<%= repo %>",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "<%= this.slug(name) %>",
    "lad",
    "lass"
  ],
  "license": "Unlicensed",
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "xo --fix",
        "git add"
      ],
      "*.md,!test/snapshots/**/*.md,!test/**/snapshots/**/*.md,!locales/README.md": [
        "remark . -qfo",
        "git add"
      ],
      "package.json": [
        "fixpack",
        "git add"
      ]
    }
  },
  "main": "index.js",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "private": true,
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ladjs/lad"
  },
  "scripts": {
    "build": "nps build",
    "coverage": "nps coverage",
    "lint": "nps lint",
    "precommit": "nps precommit",
    "pretest": "nps pretest",
    "start": "nps",
    "test": "nps test",
    "test-coverage": "nps test-coverage",
    "watch": "nps watch"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ],
    "overrides": [
      {
        "files": [
          "test/*.js",
          "test/**/*.js"
        ]
      },
      {
        "plugins": [
          "compat"
        ],
        "rules": {
          "compat/compat": "error"
        },
        "settings": {
          "polyfills": [
            "fetch"
          ]
        },
        "files": [
          "assets/js/*.js",
          "assets/js/**/*.js"
        ],
        "envs": [
          "browser"
        ]
      }
    ]
  }
}
