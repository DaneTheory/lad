{
  "name": "lad",
  "description": "<%= description %>",
  "version": "0.0.1",
  "author": "<%= author %> <<%= email %>> (<%= website %>)",
  "ava": {
    "verbose": true,
    "require": [
      "./test/global.js"
    ],
    "files": [
      "test/*.js",
      "test/**/*.js",
      "test/**/**/*.js",
      "!test/global.js"
    ]
  },
  "bugs": {
    "url": "<%= repo %>/issues",
    "email": "<%= email %>"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "<%= author %> <<%= email %>> (<%= website %>)"
  ],
  "dependencies": {
    "@ladjs/agenda": "^0.0.5",
    "@ladjs/api": "^0.2.6",
    "@ladjs/assets": "^0.0.2",
